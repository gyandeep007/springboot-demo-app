pipeline{
       agent any{
        stages{
          stage('Init'){
             steps{
                echo 'Hi this is init step'
             }
          }
          stage('Build'){
              steps{
                echo 'Hi this is build step'
              }
          }
          stage('deploy'){
              steps{
                echo 'Hi this is deploy step'
              }
          }
          stage('deploy'){
              steps{
                echo 'Hi this is deploy step'
              }
          }
        }
      }
}
